{
  "name": "magic-console-logger",
  "displayName": "Magic Console Logger",
  "description": "Quickly add, manage, and clean up console.log statements for debugging",
  "version": "1.0.0",
  "publisher": "magic-logger",
  "author": "trunglq",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/magic-console-logger"
  },
  "bugs": {
    "url": "https://github.com/your-username/magic-console-logger/issues"
  },
  "homepage": "https://github.com/your-username/magic-console-logger#readme",
  "keywords": [
    "console",
    "logging",
    "debug",
    "javascript",
    "typescript",
    "development"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Debuggers"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "magicConsoleLogger.insertLog",
        "title": "Insert Magic Log"
      },
      {
        "command": "magicConsoleLogger.deleteAllLogs",
        "title": "Delete All Magic Logs"
      },
      {
        "command": "magicConsoleLogger.commentAllLogs",
        "title": "Comment All Magic Logs"
      },
      {
        "command": "magicConsoleLogger.uncommentAllLogs",
        "title": "Uncomment All Magic Logs"
      }
    ],
    "keybindings": [
      {
        "command": "magicConsoleLogger.insertLog",
        "key": "ctrl+alt+l",
        "mac": "ctrl+alt+l",
        "when": "editorTextFocus"
      },
      {
        "command": "magicConsoleLogger.deleteAllLogs",
        "key": "shift+ctrl+alt+d",
        "mac": "shift+ctrl+cmd+d",
        "when": "editorTextFocus"
      },
      {
        "command": "magicConsoleLogger.commentAllLogs",
        "key": "shift+ctrl+alt+c",
        "mac": "shift+ctrl+cmd+c",
        "when": "editorTextFocus"
      },
      {
        "command": "magicConsoleLogger.uncommentAllLogs",
        "key": "shift+ctrl+alt+u",
        "mac": "shift+ctrl+cmd+u",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "@types/babel__traverse": "^7.20.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0"
  }
}
